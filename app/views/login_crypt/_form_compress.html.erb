<script type="text/javascript">function login_crypt_form(e){var t=<%= login_crypt_sessao_seq %>;var n=[];$.each(t,function(t,r){n[t]=e.split("")[r];if(e.split("")[r]==undefined)n[t]=" "});if(t.length<e.length)for(i=t.length;i<e.length;i++){n[i]=e[i]}return n.join("")}login_crypt_seletor_form="<%= seletor_form %>";login_crypt_input_crypt="<%= login_crypt_sessao_inputs %>".split(",");$(login_crypt_seletor_form).on("submit",function(e){e.preventDefault();var t=$(this).find("input");t.each(function(){if(login_crypt_input_crypt.indexOf($(this).attr("name"))>-1)$(this).val(login_crypt_form($(this).val()))});$(this).off("submit").submit()})</script>